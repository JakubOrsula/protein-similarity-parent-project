[Unit]
Description=Protein Search Service Manager
After=network.target

[Service]
WorkingDirectory=/home/ubuntu/protein-search-deployment
User=ubuntu
Environment="LD_LIBRARY_PATH=/home/jakub/git/school/diplomka/diplomka_impl/JOIntegration/src/main/resources/lib"
ExecStart=/usr/bin/java -jar JOIntegration-with-dependencies.jar --run checkGesamtLibPresence
SuccessExitStatus=143
Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target
